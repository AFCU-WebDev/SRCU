@using SRCUBagTracking.Models

<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="shortcut icon icon" type="image/ico" href="@Url.Content("~/Content/img/icons/SRCU.ico")" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title </title>
    <!-- style bundles -->
    @Styles.Render("~/Content/css")
    @Styles.Render("~/jQuery/theme/css")
    @Styles.Render("~/Content/fontawesome4")
</head>

<body>
    <div class="container-fluid container-fluid-customSize">

        @* --- body of each page----------------------------------------------------------------------------------------------------------------- *@
        <div class="srcu-body">
            @RenderBody()
        </div>

        <footer class="margin-bottom"></footer>

    </div>

    <!-- all minified/bundled Scripts -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/script")
    @Scripts.Render("~/bundles/maskInput")
    @RenderSection("scripts", required: false)

</body>
</html>




