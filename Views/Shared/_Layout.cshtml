@using SRCUBagTracking.Models

<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="shortcut icon icon" type="image/ico" href="@Url.Content("~/Content/img/icons/SRCU.ico")" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title </title>
    <!-- style bundles -->
    @Styles.Render("~/Content/css")

    @Styles.Render("~/jQuery/theme/css")
    @Styles.Render("~/Content/fontawesome4")

    <!-- all minified/bundled Scripts -->
    @*@Scripts.Render("~/bundles/jquery")*@
    <script src="https://code.jquery.com/jquery-2.2.4.js" integrity="sha256-iT6Q9iMJYuQiMWNd9lDyBUStIq/8PuOW33aOqmvFpqI=" crossorigin="anonymous"></script>
    @*<script src="https://code.jquery.com/jquery-migrate-3.0.0.js"></script>*@
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/script")
    @Scripts.Render("~/bundles/maskInput")

    @RenderSection("scripts", required: false)

</head>
<body>
    <div class="container-fluid ">
        <div class="row">
            @* ---logo header--------------------------------------------------------------------------------------------------------------------- *@
            <div id="logoHeader">
                <div class="container">
                    <div class="navbar-header">
                        <div class="col-md-12">
                            <img src="~/Content/img/logos/SRCULogo.png" width="200" class="appleLogo" />
                            @*<img src="~/Content/img/logos/apple-logo_192x115.png" width="150" class="appleLogo" />*@
                           @* @Html.ActionLink("SRCU", "Index", "Dashboard", new { area = "Admin" }, new { @class = "srcu-brand" })*@
                            @*<span class="srcu-brand">SRCU</span>*@
                        </div>
                    </div>
                </div>
            </div>
            @* --- Navigation bar begin ------------------------------------------------------------------------------------------------------------*@
                    
            @* --- Navigation bar End---------------------------------------------------------------------------------------------------------------- *@


            @* --- body of each page----------------------------------------------------------------------------------------------------------------- *@
            <div class="srcu-body">
                @RenderBody()
            </div>

            <footer class="margin-bottom"></footer>

        </div>
        @* ------end of row -------------------------------------------------------------------------------------------------------------------------- *@
    </div>
    @* ------end of container-Fluid ------------------------------------------------------------------------------------------------------------------ *@
</body>
</html>

