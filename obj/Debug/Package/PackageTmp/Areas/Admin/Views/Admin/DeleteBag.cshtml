@model SRCUBagTracking.Models.Bag

@{
    ViewBag.Title = "DeleteBag";
    Layout = null;
}
<!-- Modal -->

    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title bold red bigger" id="myModalLabel">Delete Alert</h4>
    </div>
    <div class="modal-body">
        <h4>Are you sure you want to delete this bag?</h4>
    </div>
    <div class="modal-footer" style="position:relative;">
        
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

    <div class="form-actions no-color" style="position:absolute;">
        <input type="submit" value="Delete" class="btn btn-primary" />

    </div>
        }
        <button type="button" class="btn btn-default " data-dismiss="modal" >Cancel</button>
     </div>




   



